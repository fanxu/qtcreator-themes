<?xml version="1.0" encoding="UTF-8"?>
<!--
     This color scheme is based on Xcode Monokai theme on Hugo Doria's github:
     https://github.com/hdoria/xcode-themes

     Modified by G. Cross, July 2014.
-->
<style-scheme version="1.0" name="Monokai-X">
  <style name="AddedLine" foreground="#00aa00"/>
  <style name="Comment" foreground="#75715e"/>
  <style name="CurrentLine" background="#464646"/>
  <style name="CurrentLineNumber" foreground="#82817f" bold="true"/>
  <style name="DiffFile" foreground="#8484f3"/>
  <style name="DiffLocation" foreground="#0084ff"/>
  <style name="DisabledCode" foreground="#a0a0a4" italic="true"/>
  <style name="Doxygen.Comment" foreground="#696556"/>
  <style name="Doxygen.Tag" foreground="#98927c"/>
  <style name="Field" bold="true" foreground="#66d9ef"/>
  <style name="Function" foreground="#fd971f"/>
  <style name="Keyword" foreground="#f92672"/>
  <style name="Label" foreground="#e76000"/>
  <style name="LineNumber" foreground="#6b6a68" background="#3e3f3a"/>
  <style name="Link" foreground="#f92672"/>
  <style name="Local" foreground="#66d9ef"/>
  <style name="Number" foreground="#fd971f"/>
  <style name="Occurrences" foreground="#000000" background="#616161"/>
  <style name="Occurrences.Rename" foreground="#000000" background="#333236"/>
  <style name="Occurrences.Unused" foreground="#808000"/>
  <style name="Operator" foreground="#e4e4e4"/>
  <style name="Parentheses" foreground="#e4e4e4"/>
  <style name="Preprocessor" foreground="#ae80ff" bold="true"/>
  <style name="PrimitiveType" foreground="#f92672" bold="false"/>
  <style name="RemovedLine" foreground="#ff0000"/>
  <style name="SearchResult" foreground="#000000" background="#646464"/>
  <style name="SearchScope" foreground="#000000" background="#f8fafc"/>
  <style name="Selection" background="#333236"/>
  <style name="Static" foreground="#ae80ff"/>
  <style name="String" foreground="#e7dc74"/>
  <style name="Text" foreground="#e4e4e4" background="#21221E"/>
  <style name="Type" foreground="#a6e22e"/>
  <style name="VirtualMethod" italic="true" foreground="#fd971f"/>
  <style name="VisualWhitespace" foreground="#646482"/>

  <!-- QML settings left untouched -->
  <style name="QmlTypeId" foreground="#ff8bff"/>
  <style name="Binding" foreground="#ff5555"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>
  <style name="QmlExternalId" foreground="#aaaaff" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#aaaaff" italic="true"/>
  <style name="JsScopeVar" foreground="#8888ff" italic="true"/>
  <style name="JsImportVar" foreground="#8888ff" italic="true"/>
  <style name="JsGlobalVar" foreground="#8888ff" italic="true"/>

  <!-- Diff settings left untouched -->
  <style name="DiffFileLine" foreground="#404040" background="#dcb178"/>
  <style name="DiffContextLine" foreground="#404040" background="#97bac7"/>
  <style name="DiffSourceLine" background="#8c2d2d"/>
  <style name="DiffSourceChar" background="#c34141"/>
  <style name="DiffDestLine" background="#277027"/>
  <style name="DiffDestChar" background="#339d33"/>
</style-scheme>
